  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "firebase emulators:start --only functions,hosting",
    "deploy": "npm run deploy:all",
    "deploy:all": "npm run build && npm run copy-files && firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "npm run build && npm run copy-files && firebase deploy --only functions",
    "copy-files": "npm run copy:server && npm run copy:next && npm run copy:env",
    "copy:server": "cp server.js functions/",
    "copy:next": "cp -r .next functions/",
    "copy:env": "cp .env.local functions/.env.local 2>/dev/null || true",
    "import:clients": "tsx scripts/import-youth-works-clients.ts",
    "add:test-client": "tsx scripts/add-test-client.ts"
  },
